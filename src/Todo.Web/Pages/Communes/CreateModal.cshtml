@page
@using Todo.Localization
@using Todo.Web.Pages.Communes
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model CreateModalModel
@inject IStringLocalizer<TodoResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="Commune" asp-page="/Communes/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["NewCommune"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>