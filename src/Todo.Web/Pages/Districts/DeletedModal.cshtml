@page
@using Todo.Localization
@using Todo.Web.Pages.Districts
@using Microsoft.Extensions.Localization
@using Todo.Permissions
@using Microsoft.AspNetCore.Authorization
@model IndexModel
@inject IStringLocalizer<TodoResource> L
@inject IAuthorizationService AuthorizationService
@section scripts
    {
    <abp-script src="/Pages/Districts/Index.js" />
}
<abp-card>
    <abp-card-header>
        <abp-row>
            <abp-column size-md="_6">
                <abp-card-title>@L["Districts Deleted"]</abp-card-title>
            </abp-column>
        </abp-row>
    </abp-card-header>
    <abp-card-body>
        <abp-table striped-rows="true" id="DistrictsDeletedTable"></abp-table>
    </abp-card-body>
</abp-card>